    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.1",
    "gh-pages": "^2.2.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts-ts": "^3.1.0",
