{
  "name": "@daniel.neuweiler/react-lib-module",
  "version": "0.1.0-alpha.13",
  "description": "This project represents my standard library for generic react components that I use in various other projects.",
  "author": "Daniel Neuweiler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xSNOWM4Nx/react-lib-module.git"
  },
  "bugs": {
    "url": "https://github.com/xSNOWM4Nx/react-lib-module/issues"
  },
  "homepage": "https://github.com/xSNOWM4Nx/react-lib-module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "directories": {
    "build": "build"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc && npm.cmd run copy:styles",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "devbuild_react-flight-tracker": "npm.cmd run build && npm.cmd run copy:packagedata && npm.cmd run copy2:react-flight-tracker",
    "copy:packagedata": "copyfiles package.json README.md build",
    "copy:styles": "copyfiles -u 1 src/styles/* build",
    "copy2:react-flight-tracker": "copyfiles build/**/* ./../react-flight-tracker/node_modules/@daniel.neuweiler/react-lib-module"
  },
  "dependencies": {
    "@daniel.neuweiler/ts-lib-module": "^0.1.0-alpha.16",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.2",
    "@mui/icons-material": "^5.2.0",
    "@svgr/rollup": "^6.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-window": "^1.8.6",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-window": "^1.8.5"
  }
}